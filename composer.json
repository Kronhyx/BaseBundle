{
  "name": "kronhyx/base-bundle",
  "type": "symfony-bundle",
  "description": "Custom symfony functions",
  "keywords": [
    "symfony",
    "php",
    "framework",
    "bundle",
    "performance",
    "api"
  ],
  "homepage": "https://kronhyx.github.io/BaseBundle/",
  "license": "MIT",
  "authors": [
    {
      "name": "Randy Téllez Galán",
      "email": "kronhyx@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "kronhyx@gmail.com",
    "issues": "https://github.com/Kronhyx/BaseBundle/issues",
    "docs": "https://kronhyx.github.io/BaseBundle/"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.3.0",
    "doctrine/doctrine-bundle": "~1.2",
    "doctrine/orm": "~2.3",
    "symfony/dependency-injection": "~2.3|~3.0",
    "symfony/event-dispatcher": "~2.3|~3.0",
    "symfony/framework-bundle": "~2.3|~3.0",
    "symfony/translation": "~2.3|~3.0",
    "symfony/twig-bundle": "~2.3|~3.0",
    "symfony/validator": "~2.3|~3.0",
    "twig/extensions": "~1.0",
    "twig/twig": "~1.14,>=1.14.2|~2.0"
  },
  "require-dev": {
    "psr/log": "~1.0",
    "symfony/yaml": "~2.3|~3.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "Kronhyx\\BaseBundle": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    }
  }
}
